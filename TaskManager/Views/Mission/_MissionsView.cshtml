@model TaskManager.Models.MissionsViewModel


<div class="panel panel-default">
    <div class="panel-body">
        @foreach (var mission in Model.Missions)
        {
            <div class="checkbox">
                <label><input type="checkbox" > @mission.Description</label>
            </div>
        }
    </div>
</div>

